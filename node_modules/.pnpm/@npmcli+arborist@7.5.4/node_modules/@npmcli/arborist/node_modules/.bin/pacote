#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/pacote@18.0.6/node_modules/pacote/bin/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/pacote@18.0.6/node_modules/pacote/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/pacote@18.0.6/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/pacote@18.0.6/node_modules/pacote/bin/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/pacote@18.0.6/node_modules/pacote/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/pacote@18.0.6/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pacote@18.0.6/node_modules/pacote/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../../pacote@18.0.6/node_modules/pacote/bin/index.js" "$@"
fi
