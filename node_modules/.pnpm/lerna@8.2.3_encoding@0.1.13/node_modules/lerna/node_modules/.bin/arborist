#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules/@npmcli/arborist/bin/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules/@npmcli/arborist/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules/@npmcli/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules/@npmcli/arborist/bin/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules/@npmcli/arborist/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules/@npmcli/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/@npmcli+arborist@7.5.4/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../@npmcli+arborist@7.5.4/node_modules/@npmcli/arborist/bin/index.js" "$@"
else
  exec node  "$basedir/../../../../../@npmcli+arborist@7.5.4/node_modules/@npmcli/arborist/bin/index.js" "$@"
fi
