#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/conventional-recommended-bump@7.0.1/node_modules/conventional-recommended-bump/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/conventional-recommended-bump@7.0.1/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/conventional-recommended-bump@7.0.1/node_modules/conventional-recommended-bump/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/conventional-recommended-bump@7.0.1/node_modules:/Users/city/Desktop/webCode/monorepo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../conventional-recommended-bump@7.0.1/node_modules/conventional-recommended-bump/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../conventional-recommended-bump@7.0.1/node_modules/conventional-recommended-bump/cli.js" "$@"
fi
